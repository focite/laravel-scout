{
  "name": "focite/laravel-scout-elastic",
  "description": "Elastic Driver for Laravel Scout",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "elasticsearch/elasticsearch": "^7.11",
    "laravel/scout": "^9.4.12"
  },
  "suggest": {
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.11.0)."
  },
  "autoload": {
    "psr-4": {
      "Focite\\Scout\\Elastic\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Focite\\Scout\\Elastic\\LaravelScoutElasticProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
